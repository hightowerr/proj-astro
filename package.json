{
  "name": "agentic-coding-starter-kit",
  "version": "1.1.2",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm run db:migrate && next build",
    "build:ci": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "npx tsx scripts/setup.ts",
    "env:check": "node -e \"require('./src/lib/env.ts').checkEnv()\" || echo 'Run with tsx: npx tsx -e \"import { checkEnv } from './src/lib/env'; checkEnv();\"'",
    "test": "vitest",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:dev": "drizzle-kit push",
    "db:reset": "drizzle-kit drop && drizzle-kit push"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.125",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@stripe/react-stripe-js": "2.9.0",
    "@stripe/stripe-js": "4.10.0",
    "@vercel/blob": "^2.0.1",
    "ai": "^5.0.123",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "drizzle-orm": "^0.44.7",
    "libphonenumber-js": "1.12.36",
    "lucide-react": "^0.539.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "pg": "^8.17.2",
    "postgres": "^3.4.8",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "^10.1.0",
    "sonner": "^2.0.7",
    "stripe": "16.12.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "latest",
    "@types/node": "^20.19.30",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.7",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "shadcn": "^3.7.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.5",
      "@types/react-dom": "19.2.3"
    }
  }
}
